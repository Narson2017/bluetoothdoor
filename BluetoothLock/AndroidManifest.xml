<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.park"
    android:versionCode="1"
    android:versionName="1.0" >

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="org.park" >
    </instrumentation>

    <uses-sdk android:minSdkVersion="7" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/company_name"
        android:theme="@android:style/Theme.Light" >
        <uses-library android:name="android.test.runner" />

        <activity android:name="org.park.entrance.NavigateActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bluetooth.box.BoxActivity" >
        </activity>
        <activity android:name="com.bluetooth.authorize.LoginActivity" >
        </activity>
        <activity android:name="org.park.prefs.settingActivity" />
        <activity android:name="org.park.devlist.DevlstActivity" />
        <activity android:name="org.park.boxlst.BoxlstActivity" />
        <activity android:name="org.park.pairpsw.AccountActivity" />
    </application>

</manifest>